{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "prettier": true,
    "max-line-length": [
      true,
      100
    ],
    "no-console": true,
    "quotemark": [
      true,
      "single"
    ]
  },
  "rulesDirectory": ["tslint-plugin-prettier"]
}
